    "preact": "^10.19.3",
    "ua-parser-js": "^1.0.37"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.23",
    "@modyfi/vite-plugin-yaml": "^1.1.0",
    "@preact/preset-vite": "^2.8.1",
    "@types/chrome": "^0.0.256",
    "@types/jest": "^29.5.11",
